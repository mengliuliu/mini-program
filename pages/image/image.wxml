<text>pages/image/image.wxml</text>

<!-- 1. image 的基本使用 -->
<!-- 
  重点：
    1. image 组件可以写成单标签，也可以写成双标签
    2. image 组件默认有自己的大小：320 x 240
    3. image 组件是一个行内块元素（inline-block）
 -->
 <!-- <image/> -->
 <text>哈哈哈</text>‘

 <!-- 2. src: 本地路径（相对路径/绝对路径）/远程地址 -->
<image src="/assests/imgs/logo.png"></image>

<!-- 补充：相册中的图片 -->
<image src="{{ imagePath }}"></image>
<button bindtap="handleChooseImage">选择图片</button>

<!-- 
      3. bindload: 监听图片加载完成
         lazy-load: 图片懒加载
 -->
<image src="data:image/jpeg;base64,"
       wx:for="{{10}}"
       bindload="handleImgLoad"
       lazy-load>
</image>

<!-- 4. show-menu-by-longpress: 长按图片出现识别小程序码 -->

